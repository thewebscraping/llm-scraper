{
  "domain": "cointelegraph.com",
  "lang": "en",
  "type": "article",
  "cleanup": [
    "script",
    "style",
    "noscript",
    "iframe"
  ],
  "title": {
    "selector": [
      "h1.post__title",
      "h1.post-title",
      "h1"
    ]
  },
  "description": {
    "selector": [
      ".post__lead",
      ".post-lead"
    ]
  },
  "content": {
    "selector": [
      ".post-content",
      ".post__content",
      "article"
    ],
    "cleanup": [
      ".ads-wrapper",
      ".newsletter-form",
      ".related-posts",
      ".social-share",
      ".popup",
      "[class*='sponsor']",
      "[class*='promoted']"
    ]
  },
  "authors": {
    "selector": [
      "//a[@rel='author']",
      ".post-meta__author-name",
      ".author-name"
    ],
    "all": true
  },
  "date_published": {
    "selector": [
      "//time[@datetime]/@datetime",
      "//meta[@property='article:published_time']/@content",
      "time[datetime]",
      ".post-meta__publish-date"
    ],
    "attribute": "datetime"
  },
  "tags": {
    "selector": [
      "//a[contains(@href, '/tags/')]",
      "a[href*='/tags/']",
      ".tags__item a",
      ".post-tags a"
    ],
    "all": true
  },
  "follow_urls": {
    "selector": [
      "//a[@data-testid='editors-choice-widget-article']/@href",
      "a.editors-choice-widget__link"
    ],
    "attribute": "href",
    "all": true
  }
}