{
  "domain": "crypto.news",
  "lang": "en",
  "type": "article",
  "title": {
    "css_selector": [
      "h1.post-title",
      "h1"
    ],
    "type": "text"
  },
  "description": {
    "css_selector": [
      ".post-excerpt",
      ".entry-summary"
    ],
    "type": "text"
  },
  "content": {
    "css_selector": [
      ".post-content",
      ".entry-content",
      "article"
    ],
    "type": "html"
  },
  "authors": {
    "css_selector": [
      ".author-name",
      "[rel='author']"
    ],
    "type": "text",
    "all": true
  },
  "date_published": {
    "css_selector": [
      "time[datetime]",
      ".post-date",
      "meta[property='article:published_time']"
    ],
    "type": "text"
  },
  "tags": {
    "css_selector": [
      ".post-tags a",
      "a[rel='tag']"
    ],
    "type": "text",
    "all": true
  },
  "cleanup": [
    ".ads",
    ".related-posts",
    ".newsletter",
    ".social-share"
  ]
}