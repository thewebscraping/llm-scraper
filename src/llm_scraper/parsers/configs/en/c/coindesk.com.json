{
  "domain": "coindesk.com",
  "lang": "en",
  "type": "article",
  "cleanup": [
    "script",
    "style",
    "noscript",
    "iframe"
  ],
  "title": {
    "selector": [
      "h1.article-hero-headline",
      "h1.headline",
      "h1"
    ]
  },
  "description": {
    "selector": [
      ".article-hero-desc",
      ".at-subheadline"
    ]
  },
  "content": {
    "selector": [
      "//main",
      ".article-body",
      ".at-content",
      "article"
    ],
    "cleanup": [
      ".at-ad",
      ".newsletter-signup",
      ".related-articles",
      ".social-share",
      ".ads-center",
      "[class*='sponsor']",
      "[class*='promoted']"
    ]
  },
  "authors": {
    "selector": [
      "//a[@rel='author']",
      ".at-authors a",
      ".article-hero-authors a"
    ],
    "all": true
  },
  "date_published": {
    "selector": [
      "//time[@datetime]/@datetime",
      "//meta[@property='article:published_time']/@content",
      "time[datetime]",
      ".at-created"
    ],
    "attribute": "datetime"
  },
  "tags": {
    "selector": [
      "//a[@rel='tag']",
      ".at-tags a",
      ".article-tags a"
    ],
    "all": true
  }
}