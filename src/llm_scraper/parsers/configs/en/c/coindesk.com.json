{
  "domain": "coindesk.com",
  "lang": "en",
  "type": "article",
  "title": {
    "selector": [
      "h1.article-hero-headline",
      "h1.headline",
      "h1"
    ],
    "type": "text"
  },
  "description": {
    "selector": [
      ".article-hero-desc",
      ".at-subheadline"
    ],
    "type": "text"
  },
  "content": {
    "selector": [
      "//main",
      ".article-body",
      ".at-content",
      "article"
    ],
    "type": "html"
  },
  "authors": {
    "selector": [
      "//a[@rel='author']",
      ".at-authors a",
      ".article-hero-authors a"
    ],
    "type": "text",
    "all": true
  },
  "date_published": {
    "selector": [
      "//time[@datetime]/@datetime",
      "//meta[@property='article:published_time']/@content",
      "time[datetime]",
      ".at-created"
    ],
    "type": "text",
    "attribute": "datetime"
  },
  "tags": {
    "selector": [
      "//a[@rel='tag']",
      ".at-tags a",
      ".article-tags a"
    ],
    "type": "text",
    "all": true
  },
  "cleanup": [
    ".at-ad",
    ".newsletter-signup",
    ".related-articles",
    ".social-share",
    ".ads-center"
  ]
}