{
  "domain": "bitcoinmagazine.com",
  "lang": "en",
  "type": "article",
  "title": {
    "selector": [
      "h1.article-title",
      "h1.entry-title",
      "h1"
    ]
  },
  "description": {
    "selector": [
      ".article-excerpt",
      ".entry-summary"
    ]
  },
  "content": {
    "cleanup": [
      ".ads",
      ".advertisement",
      ".related-posts",
      ".newsletter",
      ".social-share",
      "[class*='sponsor']",
      "[class*='promoted']"
    ],
    "selector": [
      ".article-content",
      ".entry-content",
      "article"
    ]
  },
  "authors": {
    "all": true,
    "selector": [
      ".author-name",
      "[rel='author']"
    ]
  },
  "date_published": {
    "selector": [
      "time[datetime]",
      ".article-date",
      "meta[property='article:published_time']"
    ]
  },
  "tags": {
    "all": true,
    "selector": [
      ".article-tags a",
      "a[rel='tag']"
    ]
  },
  "cleanup": [
    "script",
    "style",
    "noscript",
    "iframe",
    ".advertisement",
    ".related-articles",
    ".newsletter",
    ".social-share"
  ]
}
