{
  "domain": "baoangiang",
  "lang": "vi",
  "type": "article",
  "title": {
    "selector": ".detail-title"
  },
  "description": {
    "selector": ".detail-desc"
  },
  "content": {
    "cleanup": [
      ".ads",
      ".advertisement",
      ".related-posts",
      ".newsletter",
      ".social-share",
      "[class*='sponsor']",
      "[class*='promoted']"
    ],
    "selector": "#newscontents"
  },
  "date_published": {
    "selector": ".pubdate"
  },
  "tags": {
    "all": true,
    "selector": ".detail-tag .tag_item"
  },
  "topics": {
    "all": true,
    "selector": ".breadcrumb a"
  },
  "cleanup": [
    "script",
    "style",
    "noscript",
    "iframe",
    ".VCSortableInPreviewMode",
    "img[src*='icon-']"
  ]
}
