{
  "domain": "znews.vn",
  "lang": "vi",
  "type": "article",
  "title": {
    "selector": "the-article-title"
  },
  "content": {
    "cleanup": [
      ".ads",
      ".advertisement",
      ".related-posts",
      ".newsletter",
      ".social-share",
      "[class*='sponsor']",
      "[class*='promoted']"
    ],
    "selector": "the-article-body"
  },
  "main_points": {
    "selector": "the-article-summary"
  },
  "authors": {
    "all": true,
    "selector": [
      "the-article-source-url",
      "cms-source"
    ]
  },
  "sitemaps": [
    "https://znews.vn/sitemap/sitemap-news.xml"
  ],
  "cleanup": [
    "script",
    "style",
    "noscript",
    "iframe"
  ]
}
